{
  "name": "push-server",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "local": "dotenv -e ./dotenv/.env -- nodemon 'src' --exec 'ts-node-dev' src -e ts,graphql",
    "dev": "ts-node-dev --respawn --transpile-only src",
    "start": "ts-node src",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "introspect": "npx prisma introspect",
    "generate": "npx prisma generate",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "migrate:init": "rm -rf prisma/migrations && npx prisma migrate save --name init --experimental",
    "migrate:save": "dotenv -e ./dotenv/.env -- npx prisma migrate save --experimental",
    "migrate:up": "dotenv -e ./dotenv/.env -- npx prisma migrate up --experimental",
    "migrate:down": "dotenv -e ./dotenv/.env -- npx prisma migrate down 1 --experimental",
    "migrate:dev": "dotenv -e ./dotenv/dev.env -- npx prisma migrate up --experimental",
    "migrate:prod": "dotenv -e ./dotenv/prod.env -- npx prisma migrate up --experimental",
    "migrate:test": "dotenv -e ./dotenv/test.env -- npx prisma migrate up --experimental",
    "postinstall": "link-module-alias"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/syun1224/push-server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/syun1224/push-server/issues"
  },
  "homepage": "https://github.com/syun1224/push-server#readme",
  "devDependencies": {
    "@prisma/cli": "^2.9.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/graphql-iso-date": "^3.4.0",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-typescript": "^10.0.0",
    "eslint-plugin-graphql": "^4.0.0",
    "eslint-plugin-import": "^2.22.0",
    "nodemon": "^2.0.4",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@nexus/schema": "^0.16.0",
    "@prisma/client": "^2.9.0",
    "apollo-server-express": "^2.17.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.3.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-subscriptions": "^1.1.0",
    "nexus-plugin-prisma": "^0.21.0"
  },
  "_moduleAliases": {
    "~": "src"
  }
}
